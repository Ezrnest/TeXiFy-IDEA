<templateSet group="LaTeX">
    <template name="fig" value="\begin{figure}&#10;    \centering&#10;    \includegraphics{$FIGURE$}&#10;    \caption{$CAPTION$}&#10;    \label{fig:$LABEL$}&#10;\end{figure}&#10;$END$"
              toReformat="true" toShortenFQNames="true" key="livetemplate.latex.fig.description"
              resource-bundle="messages.TexifyBundle">
        <variable name="FIGURE" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="CAPTION" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="LABEL" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="false"/>
        </context>
    </template>

    <template name="tab" value="\begin{table}&#10;    \centering&#10;    \caption{$CAPTION$}&#10;    \label{tab:$LABEL$}&#10;    \begin{tabular}{$COLUMNS$}&#10;        $END$&#10;    \end{tabular}&#10;\end{table}&#10;$END$"
              toReformat="false" toShortenFQNames="true" key="livetemplate.latex.tab.description"
              resource-bundle="messages.TexifyBundle">
        <variable name="CAPTION" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="LABEL" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="COLUMNS" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="false"/>
        </context>
    </template>

    <template name="itm" value="\begin{itemize}&#10;    \item $END$&#10;\end{itemize}" toReformat="true" toShortenFQNames="true"
              key="livetemplate.latex.itm.description" resource-bundle="messages.TexifyBundle">
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="false"/>
        </context>
    </template>

    <template name="enm" value="\begin{enumerate}&#10;    \item $END$&#10;\end{enumerate}" toReformat="false" toShortenFQNames="true"
              key="livetemplate.latex.enm.description" resource-bundle="messages.TexifyBundle">
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="false"/>
        </context>
    </template>

    <template name="\begin1" value="\begin{$NAME$}{$ARG1$}&#10;    $END$&#10;\end{$NAME$}" toReformat="false" toShortenFQNames="true"
              key="livetemplate.latex.begin1.description" resource-bundle="messages.TexifyBundle">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="ARG1" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="true"/>
        </context>
    </template>

    <template name="\begin2" value="\begin{$NAME$}{$ARG1$}{$ARG2$}&#10;    $END$&#10;\end{$NAME$}" toReformat="false" toShortenFQNames="true"
              key="livetemplate.latex.begin2.description" resource-bundle="messages.TexifyBundle">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="ARG1" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="ARG2" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="true"/>
        </context>
    </template>

    <template name="\begin3" value="\begin{$NAME$}{$ARG1$}{$ARG2$}{$ARG3$}&#10;    $END$&#10;\end{$NAME$}" key="livetemplate.latex.begin3.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="ARG1" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="ARG2" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="ARG3" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="true"/>
        </context>
    </template>

    <template name="\partl" value="\part{$TITLE$}\label{$LABEL$}$END$" key="livetemplate.latex.partl.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="LABEL" expression="latexFormatAsLabel(TITLE)" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true" />
            <option name="LATEX_MATH" value="false" />
        </context>
    </template>

    <template name="\chapl" value="\chapter{$TITLE$}\label{ch:$LABEL$}$END$" key="livetemplate.latex.chapl.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="LABEL" expression="latexFormatAsLabel(TITLE)" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true" />
            <option name="LATEX_MATH" value="false" />
        </context>
    </template>

    <template name="\secl" value="\section{$TITLE$}\label{sec:$LABEL$}$END$" key="livetemplate.latex.secl.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="LABEL" expression="latexFormatAsLabel(TITLE)" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true" />
            <option name="LATEX_MATH" value="false" />
        </context>
    </template>

    <template name="\subsecl" value="\subsection{$TITLE$}\label{subsec:$LABEL$}$END$" key="livetemplate.latex.subsecl.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="LABEL" expression="latexFormatAsLabel(TITLE)" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true" />
            <option name="LATEX_MATH" value="false" />
        </context>
    </template>

    <template name="\subsubsecl" value="\subsubsection{$TITLE$}\label{subsubsec:$LABEL$}$END$" key="livetemplate.latex.subsubsecl.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="LABEL" expression="latexFormatAsLabel(TITLE)" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true" />
            <option name="LATEX_MATH" value="false" />
        </context>
    </template>

    <template name="enq" value="\enquote{$QUOTE$} $END$" key="livetemplate.latex.enq.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="QUOTE" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="false"/>
        </context>
    </template>

    <template name="fc" value="\footcite[$PAGE$]{$SOURCE$}$END$" key="livetemplate.latex.fc.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="PAGE" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="SOURCE" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="LATEX" value="true"/>
            <option name="LATEX_MATH" value="false"/>
        </context>
    </template>

    <!--  Mathematics  -->
    <template name="sum" value="\sum_{$FROM$}^{$TO$} $END$" key="livetemplate.latex.sum.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="FROM" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="TO" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="LATEX_MATH" value="true"/>
        </context>
    </template>

    <template name="int" value="\int_{$FROM$}^{$TO$} $END$" key="livetemplate.latex.int.description" resource-bundle="messages.TexifyBundle" toReformat="false" toShortenFQNames="true">
        <variable name="FROM" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="TO" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="LATEX_MATH" value="true"/>
        </context>
    </template>
</templateSet>
