<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <internalFileTemplate name="LaTeX Source.tex"/>
        <internalFileTemplate name="LaTeX Package.sty"/>
        <internalFileTemplate name="LaTeX Document class.cls"/>
        <internalFileTemplate name="BibTeX Bibliography.bib"/>
        <internalFileTemplate name="TikZ Picture.tikz"/>

        <fileTemplateGroup implementation="nl.hannahsten.texifyidea.templates.LatexTemplatesFactory"/>
    </extensions>
</idea-plugin>
