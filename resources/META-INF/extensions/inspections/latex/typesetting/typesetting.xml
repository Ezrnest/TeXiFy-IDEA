<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexIncorrectSectionNestingInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.incorrect.nesting"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexCollapseCiteInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.collapse.cite.commands"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexEnDashInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.en.dash.in.number.ranges"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexCdotInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.use.of.instead.of.cdot"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexXInsteadOfTimesInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.use.of.x.instead.of.times"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexVerticallyCenteredColonInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.vertically.uncentered.colon"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexQedHereInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.insert.qedhere.in.trailing.displaymath.environment"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexDiacriticIJInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.dotless.versions.of.i.and.j.must.be.used.with.diacritics"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexEncloseWithLeftRightInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.enclose.high.commands.with.leftx.rightx"
                         enabledByDefault="true"
                         level="WEAK WARNING"/>
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexCiteBeforePeriodInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.citations.must.be.placed.before.interpunction"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexQuoteInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.typesetting.issues" displayName="%inspection.display.incorrectly.typeset.quotation.marks"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexMissingGlossaryReferenceInspection"
                         groupPath="%inspection.grouppath.latex" groupName="%inspection.group.code.maturity" displayName="%inspection.display.missing.glossary.reference"
                         enabledByDefault="true"
                         level="WARNING" />
    </extensions>
</idea-plugin>